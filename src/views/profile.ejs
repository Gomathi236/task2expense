<html>
    <head>
        <title>Profile</title>
        <link rel="stylesheet" href="/public/css/profile.css">
       
        <script src="https://kit.fontawesome.com/b99e675b6e.js"></script>
    </head>
    <body>
     

     
      

<div class="wrapper">
    <div class="left">
     <div>
      <i class="fa fa-user "  id="user"></i>
     </div>
     
     <br/>
      <h4><%=  user.fullname %></h4>
         
    </div>
   
    <div class="right">
        <div class="info">
            <h3>Information</h3>
            <div class="info_data">
                 <div class="data">
                   
                   <div>
                    <label><b>NAME:</b></label>
                   </div>
                  <br/>
                  <div>
                    <h4><%=  user.fullname %></h4>
                  </div>
                  <br/>
                    <div>
                      <label><b>EMAIL:</b></label>
                    </div>   
                       
                 <br/>
                    <div>
                      <h4><%= user.email %></h4>
                    </div>  
                   

                   
                 </div>
                 
            </div>
        </div>
      
     
      
        
    </div>
</div>
       
    </body>
</html>